# Custom and useful aliases!
alias ll="ls -alt"
alias cdansible="cd /etc/ansible"
alias pls="sudo"
alias upd-upg="sudo apt -y update && sudo apt -y upgrade && sudo apt -y full-upgrade && sudo apt -y autoremove"
alias shtdwn="shutdown -h"
alias rstrt="shutdown -r"
alias shtdwn-now="shtdwn now"
alias rstrt-now="restart now"
